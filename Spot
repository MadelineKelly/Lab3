package lab_3;

/**
 *
 * @author Madeline
 */
public class Spot {

  private boolean visited;
  private int count;
  
  Spot()
  {
    visited = false;
    count = 0;
  }
  
  public void visit()
  {
    visited = true;
  }
  
  public void incCount()
  {
    count = count + 1;
  }
  
  public int getCount()
  {
    return count;
  }
    
  public boolean getVisited()
  {
    return visited;
  }
      
}
