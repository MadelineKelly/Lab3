
package lab_3;

public class Spot {

  private boolean visited;
  private int count;
  
  Spot()
  {
      visited = false;
      count = 0;
  }
  
  public void visit()
  {
      visited = true;
  }
  
  public void unvisit()
  {
      visited = false;
  }
    
  public boolean getVisited()
  {
      return visited;
  }
      
}
